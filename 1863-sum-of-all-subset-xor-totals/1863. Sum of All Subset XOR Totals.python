class Solution(object):
    def subsetXORSum(self, nums):
        if len(nums) == 0 :
            return 0 
        ans = 0 

        for num in nums:
            ans |= num
            print(ans)
        
        return ans << (len(nums)-1)

        
        """
        :type nums: List[int]
        :rtype: int
        """
        