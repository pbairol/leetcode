class Solution(object):
    def twoSum(self, nums, target):
        num_map = defaultdict(int)

        for idx, num in enumerate(nums):
            if (target - num) in num_map:
                return [num_map[target-num], idx]
            num_map[num] = idx
        
        return []
        """
        :type nums: List[int]
        :type target: int
        :rtype: List[int]
        """
        