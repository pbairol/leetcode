class Solution(object):
    def isAnagram(self, s, t):
        if len(s) != len(t):
            return False
        
        mapS = defaultdict(int)
        mapT = defaultdict(int)

        for letter in s:
            if letter in mapS:
                mapS[letter] +=1
            else:
                mapS[letter] = 1
        
        for letter in t:
            if letter in mapT:
                mapT[letter] +=1
            else:
                mapT[letter] = 1
        for key in mapS:
            if mapS[key] != mapT[key]:
                return False
        return True
        
        """
        :type s: str
        :type t: str
        :rtype: bool
        """
        