class Solution(object):
    def findMaxK(self, nums):

        negNumber= set()

        for num in nums:
            if num < 0:
                negNumber.add(num)

        ans = -1
        for num in nums:
            if num > ans and (-1*num) in negNumber:
                ans = num
        return ans

        """
        :type nums: List[int]
        :rtype: int
        """
        