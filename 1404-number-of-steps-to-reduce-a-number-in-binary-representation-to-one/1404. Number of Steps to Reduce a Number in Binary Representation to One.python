class Solution(object):
    def numSteps(self, s):
        #conver to int:
        bin_in_int = int(s,2)
        operations = 0 
        while bin_in_int != 1:
            if bin_in_int % 2 == 1:
                bin_in_int +=1
            else:
                bin_in_int //=2
            operations +=1
        return operations