class Solution(object):
    def reverseStr(self, s, k):
        s_list = list(s)

        for i in range(0,len(s),2*k):
            j = (i + k -1)
            if j>= len(s):
                j = len(s)-1
            while i <= j and i<len(s):
                s_list[i],s_list[j] = s_list[j], s_list[i]
                i +=1
                j -=1
        

        return "".join(s_list)
        """
        :type s: str
        :type k: int
        :rtype: str
        """
        